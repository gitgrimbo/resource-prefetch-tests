{
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "rules": {
    "comma-dangle": "warn",
    "linebreak-style": [
      "error",
      "windows"
    ],
    "max-len": [
      "warn",
      80
    ],
    "object-curly-spacing": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "double",
      {
        "allowTemplateLiterals": true
      }
    ],
    "require-jsdoc": "warn"
  }
}
